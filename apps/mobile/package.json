  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:local": "cross-env RCT_METRO_PORT=8083 expo start --host localhost --port 8083 -c",
